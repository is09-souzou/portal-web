{
  "name": "portal-web",
  "version": "0.1.1",
  "main": "src/index.js",
  "repository": "https://github.com/is09-souzou/portal-web",
  "author": "NozomiSugiyama <rioc.sugiyama@gmail.com>",
  "license": "MIT",
  "bin": "./index.js",
  "scripts": {
    "start": "env NODE_ENV=development webpack-serve webpack.config.js",
    "start:prod": "env NODE_ENV=production webpack-serve webpack.config.js",
    "usapi": "webpack-serve webpack.config.js",
    "build": "yarn build:dev",
    "build:dev": "env NODE_ENV=development webpack --mode=development --config webpack.config.js",
    "build:prod": "env NODE_ENV=production webpack --mode=production --config webpack.config.js",
    "clean-cache": "rm -Rf ./node_modules/.cache/hard-source",
    "lint": "tslint -p ./ -c ./tslint.json './src/**/*.ts{,x}'",
    "test": "yarn lint && yarn clean-cache && yarn build",
    "size-analyze": "yarn build:prod --json | tail -n +2 | webpack-bundle-size-analyzer",
    "docs": "typedoc --excludeExternals --externalPattern '**/node_modules/**' --ignoreCompilerErrors --out ./docs/ ./src/ && echo 'include:\n  - \"_*_.html\"\n  - \"_*_.*.html\"' > docs/_config.yml",
    "deploy": "yarn lint && yarn build:prod && aws s3 sync ./assets s3://souzou-portal/ --exclude '.DS_Store' --acl public-read --cache-control 'max-age=3600' && aws s3 sync ./dist s3://souzou-portal/  --exclude '.DS_Store' --acl public-read --cache-control 'max-age=3600'",
    "webpack": "webpack"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "amazon-cognito-identity-js": "^2.0.24",
    "apollo-client": "^2.4.1",
    "aws-appsync": "^1.3.4",
    "aws-appsync-react": "^1.1.4",
    "graphql": "^14.0.0",
    "graphql-tag": "^2.9.2",
    "react": "^16.5.0",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-loadable": "^5.5.0",
    "react-markdown": "^3.5.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.4.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "v7.0.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-loadable": "^5.4.1",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-swipeable-views": "^0.12.1",
    "@types/uuid": "^3.4.4",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-styled-components": "^1.7.1",
    "connect-history-api-fallback": "^1.5.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "koa-connect": "^2.0.1",
    "ts-loader": "^5.0.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.3",
    "typescript-styled-plugin": "^0.11.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  }
}
